{
  "name": "renderer",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "link:parser": "yalc add @markwhen/parser && yalc link @markwhen/parser && npm install",
    "unlink:parser": "yalc remove @markwhen/parser && npm install"
  },
  "dependencies": {
    "@markwhen/parser": "^0.3.8",
    "@squadette/hammerjs": "^2.1.0-pre3",
    "@vuepic/vue-datepicker": "^3.6.3",
    "@vueuse/core": "^9.7.0",
    "@vueuse/integrations": "^9.7.0",
    "chrono-node": "^2.4.2",
    "lru-cache": "^7.14.1",
    "lunr": "^2.3.9",
    "luxon": "^3.1.1",
    "pinia": "^2.0.28",
    "throttle-debounce": "^5.0.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^20.0.1",
    "@types/lunr": "^2.3.4",
    "@types/luxon": "^3.1.0",
    "@types/node": "^16.11.45",
    "@types/throttle-debounce": "^5.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/head": "^1.0.22",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.29.0",
    "eslint-plugin-vue": "^9.8.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier": "^2.8.1",
    "sass": "^1.56.2",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.1",
    "vitest": "^0.25.8",
    "vue-tsc": "^1.0.13"
  }
}
